<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cursor"
    android:layout_width="@dimen/eq_pro_graph_cursor_width"
    android:layout_height="@dimen/eq_pro_graph_cursor_width"
    android:layout_gravity="left"
    android:layout_alignParentLeft="true"
    android:visibility="invisible">

    <ImageView
        android:id="@+id/cursor_back"
        android:layout_width="@dimen/eq_pro_graph_cursor_width"
        android:layout_height="@dimen/eq_pro_graph_cursor_width"
        android:layout_centerInParent="true"
        android:scaleType="fitCenter"
        android:src="@drawable/p0114_draweq" />

    <View
        android:layout_width="@dimen/eq_pro_graph_cursor_oval_width"
        android:layout_height="@dimen/eq_pro_graph_cursor_oval_width"
        android:layout_centerInParent="true"
        android:alpha="0.9"
        android:background="@drawable/oval" />
</RelativeLayout>