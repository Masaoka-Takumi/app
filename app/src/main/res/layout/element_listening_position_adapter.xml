<?xml version="1.0" encoding="utf-8"?>
    <jp.pioneer.carsync.presentation.view.widget.CheckableFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/frame"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <jp.pioneer.carsync.presentation.view.widget.AutoshrinkTextView
        android:id="@android:id/text2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left|center_vertical"
        android:minHeight="?attr/listPreferredItemHeightSmall"
        android:paddingLeft="?attr/dialogPreferredPadding"
        android:paddingRight="?attr/dialogPreferredPadding"
        android:lines="1"
        android:fontFamily="sans-serif-light"
        android:text="@string/val_057"
        android:textColor="@color/drawable_white_color"
        android:textSize="@dimen/advanced_audio_speaker_setting_menu_text_size"
        android:background="#00000000" />
    <CheckedTextView
        android:id="@android:id/text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:checkMark="?android:attr/listChoiceIndicatorSingle"
        android:checked="true"
        android:layoutDirection="ltr"
        android:gravity="left|center_vertical"
        android:minHeight="?attr/listPreferredItemHeightSmall"
        android:paddingLeft="?attr/dialogPreferredPadding"
        android:paddingRight="?attr/dialogPreferredPadding"
        android:lines="1"
        android:fontFamily="sans-serif-light"
        android:textColor="@color/drawable_white_color"
        android:textSize="23sp"
        android:clickable="false" />
</jp.pioneer.carsync.presentation.view.widget.CheckableFrameLayout>